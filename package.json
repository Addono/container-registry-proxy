{
  "name": "container-registry-proxy",
  "version": "1.1.0",
  "description": "A simple container registry proxy.",
  "main": "src/index.ts",
  "repository": "https://github.com/Addono/container-registry-proxy.git",
  "author": "Adriaan Knapen <hi@aknapen.nl>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "dev": "ts-node-dev src/index.ts"
  },
  "bin": {
    "container-registry-proxy": "./bin/index.js",
    "crp": "./bin/index.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/node": "^13.13.4",
    "husky": "^4.2.5",
    "semantic-release": "^17.0.7",
    "ts-node": "^8.9.1",
    "ts-node-dev": "^1.0.0-pre.44"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
